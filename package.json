{
  "name": "@rlj1202/concat-writable-streams",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "author": "Jisu Sim <rlj1202@gmail.com>",
  "description": "Concatenates multiple writable streams into a single ordered one",
  "keywords": [
    "typescript",
    "stream",
    "streams",
    "writable-stream",
    "writablestream",
    "concat"
  ],
  "zshy": {
    "exports": {
      "./packages.json": "./package.json",
      ".": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "zshy",
    "test": "vitest run",
    "bump": "changeset version && echo \"$(jq \".version = \\\"$(jq -r \".version\" package.json)\\\"\" jsr.json)\" > jsr.json",
    "check:type": "tsc --noEmit",
    "check:biome": "biome check",
    "check:npm": "pnpm publish --access public --dry-run --no-git-checks",
    "check:jsr": "pnpm dlx jsr publish --dry-run --allow-dirty",
    "publish:npm": "pnpm publish --access public",
    "publish:jsr": "pnpm dlx jsr publish"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@std/streams": "jsr:^1.0.14",
    "typescript": "^5.9.3",
    "vitest": "^4.0.13",
    "zshy": "^0.6.0"
  },
  "files": [
    "dist",
    "src",
    "package.json"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "types": "./dist/index.d.cts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./packages.json": "./package.json"
  },
  "packageManager": "pnpm@10.23.0",
  "dependencies": {
    "@changesets/cli": "^2.29.7"
  }
}
